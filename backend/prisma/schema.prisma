generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String @unique
  createdAt DateTime @default(now())
}

model Emocao {
  id Int @id @default(autoincrement())
  name String
  foto String
}

model Post {
  id Int @id @default(autoincrement())
  Legenda String
}

